<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0" xmlns="http://java.sun.com/xml/ns/persistence">

	<persistence-unit name="ajaxquotedata">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.openfeed.backend.vo.equity.EquityEntity</class>
		<class>com.openfeed.backend.vo.equity.MarketEntity</class>
		<class>com.openfeed.backend.vo.user.User</class>
		<class>com.openfeed.backend.vo.user.UserUIMarketEntity</class>
		<class>com.openfeed.backend.vo.userui.UserUI</class>
		<properties>
			<!-- property name="hibernate.hbm2ddl.auto" value="validate" /> -->
			<property name="hibernate.show_sql" value="false" />
			<property name="hibernate.cache.provider_class" value="org.hibernate.cache.HashtableCacheProvider" />
		</properties>
	</persistence-unit>

</persistence>
